<%- @config.each do |conf| %>
<%= conf %>
<%- end %>
<%- if @rdomain != 0 %>
rdomain <%= @rdomain %>
<%- end %>
<%- if @tunneldomain != 0 %>
tunneldomain <%= @tunneldomain %>
<%- end %>
<%- if @tunnel %>
tunnel <%= @tunnel %>
<%- end %>
<%- if @mtu %>
mtu <%= @mtu %>
<%- end %>
<%- if @dhcp %>
dhcp
<%- elsif @inet %>
inet <%= @inet %>
<%- end %>
<%- if @inet6 %>
inet6 <%= @inet6 %>
<%- end %>
<%- if @inner %>
<%= @inner %>
<%- end %>
<% @extra_commands.each do |cmd| %>
!<%= cmd %>
<% end %>
